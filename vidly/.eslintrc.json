{
   "env": {
      "node": true,
      "mongo": true
   },
   "parser": "babel-eslint",
   "rules": {
      "indent": [
         "error",
         3,
         {
            "SwitchCase": 1
         }
      ],
      "brace-style": ["error", "stroustrup"],
      "no-underscore-dangle": "off",
      "no-shadow": "off",
      "implicit-arrow-linebreak": "off"
   },
   "extends": ["airbnb-base"]
}
